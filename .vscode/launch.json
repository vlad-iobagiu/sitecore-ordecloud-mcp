{
  "version": "0.2.0",
  "configurations": [
    {
  "type": "node",
      "request": "launch",
      "name": "Debug Main",
      "program": "${workspaceFolder}/main.ts",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "outFiles": ["${workspaceFolder}/build/**/*.js"],
      "sourceMaps": true,
      "cwd": "${workspaceFolder}"
    },
    {
  "type": "node",
      "request": "launch",
      "name": "Debug Tool (ordercloud-client)",
      "program": "${workspaceFolder}/tools/ordercloud-client.ts",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "outFiles": ["${workspaceFolder}/build/**/*.js"],
      "sourceMaps": true,
      "cwd": "${workspaceFolder}"
    }
  ]
}
